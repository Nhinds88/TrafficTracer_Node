<%- include('partials/header.ejs') %>

        <form action="/search">
            <input type="text" id="keyword" value="<%=keyword%>" placeholder="Type Keyword" name="keyword" />
            <button>
                search
            </button>
            <a id="favLink" href="/displayKeywords"> See Favorites </a>
        </form>
        <% imageURLs.forEach(function(imageURLl, i){ %>
        
            <%- (i%3==0)?'<br>':""%>
            <div class="imageContainer">
                <img class="image" src='<%=imageURLl%>' width='200' height='200'>
                <img class='favoriteIcon' src='img/favorite.png' width="20">
            </div>
        <% }) %>

<%- include('partials/footer.ejs') %>